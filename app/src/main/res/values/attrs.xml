<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ListingsButton">
        <attr name="android:text" format="string" />
        <attr name="android:paddingHorizontal" format="integer" />
        <attr name="isText" format="boolean" />
        <attr name="isColorBtn" format="boolean" />
        <attr name="android:drawable" />
        <attr name="android:layout_height" />
        <attr name="android:layout_width" />
    </declare-styleable>

    <declare-styleable name="ListingsEditText">
        <attr name="labelText" format="string" />
        <attr name="errorText" format="string" />
        <attr name="android:nextFocusDown" />
        <attr name="android:imeOptions" />
    </declare-styleable>

    <declare-styleable name="ListingsSizeRadioButton">
        <attr name="usSize" format="integer" />
        <attr name="euSize" format="string" />
        <attr name="android:checked" />
    </declare-styleable>

    <declare-styleable name="ListingsIcon">
        <attr name="android:src" />
        <attr name="hasBorder" format="boolean"/>
    </declare-styleable>
</resources>